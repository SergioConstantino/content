[MODEL:model="Email", dataset="proofpoint_threat_response_raw"]
| alter XDM.Email.event_timestamp = created_at,
        XDM.Email.original_event_id = id,
        XDM.Email.threat.category = category,
        XDM.Email.threat.severity = severity,
        XDM.Email.sender = json_extract_scalar(events, "$.sender.email"),
        XDM.Email.recipients = json_extract_scalar(events, "$.recipient.email"),
        XDM.Email.message_id = json_extract_scalar(events, "$.messageId"),
        XDM.Email.delivery_timestamp = json_extract_scalar(events, "$.messageDeliveryTime.millis"),
        XDM.Email.Client.user.username = users;